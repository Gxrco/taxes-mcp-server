{
	"name": "taxes-mcp-server",
	"version": "1.0.0",
	"description": "MCP server for tax calculations deployed on Cloudflare Workers",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "tsc",
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"format": "biome format --write",
		"lint": "biome lint",
		"lint:fix": "biome lint --fix",
		"start": "wrangler dev",
		"cf-typegen": "wrangler types",
		"type-check": "tsc --noEmit",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.18.0",
		"agents": "^0.1.3",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@types/node": "^24.5.2",
		"typescript": "5.9.2",
		"wrangler": "^4.36.0"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
